---
import Layout from '../layouts/DashboardLayout.astro';
import { Dashboard } from '@krag/react-ui';

// Fetch from public directory (works at build time in Astro)
const response = await fetch(`${Astro.url.origin}/data/dashboard.json`);
const tableData = await response.json();
---

<Layout title="Users">
  <Dashboard data={tableData} showStats={false} client:load />
</Layout>
